@model List<DataModel.Model.Commom.FunctionViewModel>
<ul class="nav nav-pills nav-stacked custom-nav">
   
    @foreach (var menu in Model)
    {
        <li class="menu-list">
            <a href="#">
                <i class="lnr @menu.IconCss"></i>
                <span>@menu.Name</span>
            </a>
            @if (menu.Functions1.Count > 0)
            {
                <ul class="sub-menu-list">
                    @foreach (var menu1 in menu.Functions1)
                    {
                        <li><a href="@menu1.Url">@menu1.Name</a> </li>
                    }
                </ul>
            }
        </li>
    }
</ul>
